@model IEnumerable<duandian_test.Models.xiugairizhi>
@{
    ViewBag.Title = "rizhi";
}

<div style="padding-bottom:5px;padding-left:20px;">
    当前位置：| &gt;资产查询  &gt; 修改日志  &nbsp;&nbsp;<button type="button" class="btn btn-primary" onclick="window.location.href = '@Url.Action("Index", "Home", new { sortOrder = @ViewBag.sortOrderzhi, searchString = @ViewBag.searchzhi, zhuangtai = @ViewBag.zhuangtai, shenhejieguo = @ViewBag.shenhejieguozhi })'">返 回</button>
    <br />
</div>
<div style="padding:0px 20px;">
    <table class="table table-striped table-hover" id="test">
        <thead>
            <tr style="background-color:#e0cc5f">
                <th nowrap>
                    序号
                </th>
                <th nowrap>
                    操作
                </th>

                <th nowrap>
                    操作人
                </th>

                <th nowrap>
                    操作日期
                </th>
            </tr>
        </thead>
        <tbody>
            @{  var hangyanse = "";
              int hao = 1;
            }
            @foreach (var item in Model)
            {
                <tr class="@hangyanse">
                    <td>
                        @hao
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.caozuo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.caozuoren)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.caozuoriqi)
                    </td>
                </tr>
                hao = hao + 1;
            }
        </tbody>
    </table>

    <div class="form-group   has-error">
        <div class="col-md-3"></div>
        <div class="col-md-4">
            <p>                             
            </p>
        </div>
        <div class="col-md-5"></div>
    </div>
</div>
