            @Html.LabelFor(model => model.shenhezhuangtai, htmlAttributes: new { @class = "control-label col-md-2" })
            @if (Model.shenhezhuangtai == "通过")
            {
                <div class="col-md-6">
                    <label class="checkbox-inline">
                        <input type="radio" name="shenhezhuangtai" id="optionsRadios3"
